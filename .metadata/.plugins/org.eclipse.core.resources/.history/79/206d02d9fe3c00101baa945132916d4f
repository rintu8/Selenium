		String str1 = "a2b2c4";
		
		for(int i=0;i<str1.length;i++) {
			
			if(Character.isAlphabetic(str1.charAt(i))) {
				
				System.out.println(str1.charAt(i));
				
				
			}
			
			else {
				
				
			}
			
		}
		
		
	}

}
